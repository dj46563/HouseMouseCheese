<Window x:Class="HouseMouseCheese.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:HouseMouseCheese"
        mc:Ignorable="d"
        Title="House Mouse: Cheese" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <local:FrameDisplay/>
            <xctk:ColorPicker SelectedColorChanged="ColorPicker_SelectedColorChanged"></xctk:ColorPicker>
        </StackPanel>
    </Grid>
</Window>
